{
  "name": "iet-template",
  "version": "1.0.0",
  "description": "A template for future projects",
  "main": "index.js",
  "scripts": {
    "watch": "watchify src/main.js -t babelify -o public/bundle.js -v",
    "build": "browserify src/main.js -t babelify | uglifyjs > public/bundle.js",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d public",
    "test": "mocha --compilers js:babel-core/register"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CityOfDetroit/iet-template.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/CityOfDetroit/iet-template/issues"
  },
  "homepage": "https://github.com/CityOfDetroit/iet-template",
  "dependencies": {
    "lodash": "^4.17.4",
    "mapbox-gl": "^0.40.1",
    "moment": "^2.18.1",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babelify": "^7.3.0",
    "gh-pages": "^1.0.0",
    "should": "^11.2.1"
  }
}
